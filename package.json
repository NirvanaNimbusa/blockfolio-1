{
  "name": "blockfolio",
  "version": "0.5.2",
  "description": "cli and module for convenient access to blockfolio",
  "main": "lib/index.js",
  "preferGlobal": true,
  "bin": {
    "blockfolio": "bin/index.js"
  },
  "scripts": {
    "start": "node bin",
    "test": "ava",
    "format": "clang-format -i -style=Google --glob=bin/**/*.js",
    "lint": "eslint . --fix"
  },
  "keywords": [
    "blockfolio",
    "blockchain",
    "portfolio",
    "coin",
    "cryptocurrency"
  ],
  "author": "Max Millien",
  "license": "MIT",
  "dependencies": {
    "cli-table2": "0.x",
    "colors": "1.x",
    "debug": "3.x",
    "node-fetch": "1.x"
  },
  "devDependencies": {
    "ava": "*",
    "eslint": "3.x",
    "eslint-config-airbnb-base": "11.x",
    "eslint-plugin-import": "2.x"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmill/blockfolio.git"
  },
  "bugs": {
    "url": "https://github.com/maxmill/blockfolio/issues"
  },
  "homepage": "https://github.com/maxmill/blockfolio#readme"
}
